{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "ansible-galaxy": {
    "fileMatch": ["(^|/)(galaxy|requirements)(\\.ansible)?\\.ya?ml$", "(^|/)meta/main\\.ya?ml$"]
  },

    "packageRules": [
    {
      "versioning": "regex:^release-v?(?<major>\\d+)\\.(?<minor>\\d+)\\.(?<patch>\\d+)?$",
      "matchPackageNames": ["/.*/"]
    }
  ]
}
